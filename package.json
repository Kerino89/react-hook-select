{
  "name": "react-use-select",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "description": "React hook to create the select component.",
  "main": "dist/react-use-select.cjs.js",
  "module": "dist/react-use-select.esm.js",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-use-select.esm.js",
      "require": "./dist/react-use-select.cjs.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "hooks",
    "select",
    "typescript",
    "react-hooks"
  ],
  "author": {
    "email": "kerino89@gmail.com",
    "name": "Utkin Anton"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kerino89/react-use-select"
  },
  "scripts": {
    "build": "tsup",
    "start:example": "npm run build && npm link && npm --prefix ./example link react-use-select && npm --prefix ./example run dev",
    "lint": "eslint ./src/**/*.ts",
    "lint:fix": "eslint --fix ./src/**/*.ts",
    "test": "vitest",
    "test:cov": "vitest run --coverage",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "prepare": "husky install",
    "commit": "cz",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "react": ">=17"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/compat": "^1.1.0",
    "@eslint/js": "^9.5.0",
    "@testing-library/react": "^16.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitest/coverage-v8": "^1.6.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.5.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-release": "^24.0.0",
    "tslib": "^2.4.0",
    "tsup": "^8.1.0",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.13.1",
    "vitest": "^1.6.0"
  },
  "pnpm": {
    "overrides": {
      "eslint": "^9.5.0",
      "react": "^18.0.0"
    }
  }
}
